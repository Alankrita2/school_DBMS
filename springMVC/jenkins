pipeline{
    agent any
    stages{
        stage("compilePhase"){
            steps{
                bat "cd springMVC"
                bat "compile"
            }
        }

        stage("testPhase"){
            steps{
                bat "cd springMVC"
                bat "test"
            }
        }

        stage("packagingPhase"){
            steps{
                bat "cd springMVC"
                bat "clean package"
            }
        }
    }
}
